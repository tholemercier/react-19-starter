{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node scripts/show-deps-check.js && concurrently \"tcm src --include '**/*.module.css' --watch\" \"vite\"",
    "build": "tcm src --include '**/*.module.css' && tsc -b && vite build",
    "build:analyze": "tcm src --include '**/*.module.css' && tsc -b && vite build --mode analyze",
    "typecheck": "tsc -b --pretty false --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage --run",
    "test:open": "open coverage/index.html",
    "test:open:windows": "start coverage/index.html",
    "preview": "vite preview",
    "lint": "eslint . --cache --max-warnings=0",
    "prepare": "husky",
    "deps:check": "node scripts/deps-check.js",
    "local:up": "pnpm exec husky init && pnpm install --frozen-lockfile"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tanstack/react-query": "^5.90.21",
    "clsx": "^2.1.1",
    "jotai": "^2.17.1",
    "lodash": "^4.17.23",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router": "^7.13.0",
    "react-use": "^17.6.0",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.23",
    "@types/node": "^24.10.13",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "babel-plugin-react-compiler": "^1.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^63.0.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "msw": "^2.12.10",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^4.1.18",
    "typed-css-modules": "^0.9.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1",
    "vite-plugin-checker": "^0.12.0",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  }
}
